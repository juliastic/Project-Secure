; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Button",
"class": "DefaultButton",
"language": "GDScript",
"path": "res://helpers/default_button.gd"
}, {
"base": "EditorVCSInterface",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
}, {
"base": "DefaultButton",
"class": "NetworkNode",
"language": "GDScript",
"path": "res://network/network_node.gd"
}, {
"base": "TerminalText",
"class": "SmallTerminalText",
"language": "GDScript",
"path": "res://helpers/small_terminal_text.gd"
}, {
"base": "RichTextLabel",
"class": "TerminalText",
"language": "GDScript",
"path": "res://helpers/terminal_text.gd"
} ]
_global_script_class_icons={
"DefaultButton": "",
"GitAPI": "",
"NetworkNode": "",
"SmallTerminalText": "",
"TerminalText": ""
}

[application]

config/name="Project Secure"
run/main_scene="res://main/Main.tscn"
config/icon="res://icon.png"

[autoload]

KeyConstants="*res://helpers/key_constants.gd"
GameProgress="*res://player_progress/game_progress.gd"
TerminalCommands="*res://helpers/terminal_commands.gd"
NodeDescriptions="*res://helpers/node_descriptions.gd"
ConsoleScripts="*res://dialgoue/console_scripts.gd"
ConsoleDialogue="*res://dialgoue/console_dialogue.gd"
Requests="*res://player_progress/requests.gd"
TerminalData="*res://player_progress/terminal_data.gd"

[display]

window/size/width=1920
window/size/height=1080
mouse_cursor/custom_image="res://imgs/cursor-sample.png"
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/gif-importer/plugin.cfg", "res://addons/godot-gif-importer-main/addons/gif-importer/plugin.cfg" )

[global]

application=false
Display=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.627451, 0.631373, 0.631373, 1 )
environment/default_environment="res://default_env.tres"
